# Copy this file to .env and fill in your actual values

# Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-pro
GEMINI_TEMPERATURE=0.7
GEMINI_MAX_TOKENS=1000

# Agent Configuration
NUM_AGENTS=3
DEFAULT_TURNS=2

# Multi-Model Configuration (JSON) - Optional
# If not set, all agents will use the default GEMINI_MODEL
# Example with different models per agent:
AGENT_MODELS_CONFIG='[
  {
    "agent_id": "agent_0",
    "model": "gemini-pro",
    "temperature": 0.3,
    "personality": "You are a conservative, fact-based analyst who prioritizes accuracy and evidence."
  },
  {
    "agent_id": "agent_1", 
    "model": "gemini-pro-vision",
    "temperature": 0.9,
    "personality": "You are a creative visionary who thinks outside the box and proposes innovative solutions."
  },
  {
    "agent_id": "agent_2",
    "model": "gemini-pro",
    "temperature": 0.5,
    "personality": "You are a critical thinker who identifies potential problems and challenges assumptions."
  }
]'

# API Configuration
API_HOST=0.0.0.0
API_PORT=8000

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_DB=0
REDIS_STREAM_MAXLEN=1000

# PostgreSQL Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=password
POSTGRES_DB=polyagents
POSTGRES_ECHO=false

# Qdrant Configuration (Optional)
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_VECTOR_SIZE=384

# Logging
LOG_LEVEL=INFO

# Development
DEBUG=false 